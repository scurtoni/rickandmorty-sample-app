{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "lib": ["esnext", "dom", "dom.iterable"],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "rootDir": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "paths": {
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@state/*": ["./src/state/*"],
      "@models": ["./src/models"]
    }
  }
  // "include": ["./src", "./webpack.config.ts", "webpack.config.js"]
}
